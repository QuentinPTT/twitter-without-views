
{
    "manifest_version": 3,
    "name": "TWV",
    "description": "Delete views from Twitter",
    "version": "1.0",
    "icons": {
      "128": "img/128.png"
    },
    "content_scripts": [
      {
        "js": [
          "scripts/main.js"
        ],
        "matches": [
          "https://twitter.com/*"
        ]
      }
    ]
  }